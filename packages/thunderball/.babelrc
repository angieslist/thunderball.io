{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    [
      "transform-runtime", {
        "polyfill": false,
        "regenerator": false
      }
    ],
    "add-module-exports",
    "lodash"
  ],
  "env": {
    "development": {
      "plugins": [
        "jsx-control-statements"
      ]
    },
    "production": {
      "plugins": [
        "jsx-control-statements",
        "transform-react-constant-elements",
        "transform-react-inline-elements"
      ]
    }
  }
}
